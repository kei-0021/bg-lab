    "rimraf": "^6.1.2",
    "rollup": "4.20.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^7.3.1",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-static-copy": "^3.2.0"
  },
  "dependencies": {
    "kei-0021@issue-12@latest": "link:github/kei-0021@issue-12@latest",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-game-ui": "github:kei-0021/react-game-ui#issue-12",
    "react-router-dom": "^7.13.0",
